{
    "plugins": [
        "vue", "html"
    ],
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "vue"
    ],
    "rules": {
        "no-set-state": "off",
        // Override our default settings just for this directory
        "eqeqeq": "warn",
        "strict": "off"
    }
}
